  "scripts": {
    "start": "cross-env DBNAME=\"production.dentedlotus.sqlite\" node app.js",
    "dev-start": "cross-env DBNAME=\"dev.dentedlotus.sqlite\" nodemon app.js",
    "debug": "cross-env DBNAME=\"dev.dentedlotus.sqlite\" node --debug-brk=5858 app.js",
    "deploy": "gulp deploy",
    "webpack": "npm run compile-webpack-config && webpack && echo Webpack complete",
    "copy-static-files": "npm run copy-shims && cpy node_modules/react-draft-wysiwyg/dist/react-draft-wysiwyg.css dist && cpy node_modules/draft-js/dist/Draft.css dist && cpy index.html dist && cpy --cwd=src images/**/*.* ../dist --parents && cpy favicon.ico dist",
    "copy-shims": "cpy node_modules/whatwg-fetch/fetch.js dist && cpy node_modules/es6-shim/es6-shim.js dist",
    "compile-less": "lessc style.less dist/style.css && echo less compile complete",
    "build": "npm run compile && npm run webpack && npm run copy-static-files && npm run compile-less",
    "bundle": "npm run build && npm run copy-misc && npm run copy-server && npm run copy-client",
    "copy-bundle": "cpy --cwd=bundle **/*.* ../../dented-lotus --parents && cpy .prod.gitignore ../dented-lotus --rename=.gitignore",
    "copy-client": "cpy --cwd=dist ./**/*.* ../bundle/dist --parents",
    "copy-misc": "cpy package.prod.json bundle --rename=package.json",
    "copy-server": "cpy app/**/*.js app/**/*.sqlite common/**/*.js bundle --parents && cpy ./app.js bundle",
    "compile": "tsc -v && tsc && echo Typescript compile complete",
    "watch": "tsc -v && tsc -w",
    "test": "npm run build",
    "compile-webpack-config": "tsc webpack.config.ts",
    "prod-update": "git pull && npm run build && npm run start"
  }
}