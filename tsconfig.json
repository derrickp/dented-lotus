{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "dom.iterable"
    ],
    "moduleResolution": "node",
    "baseUrl": ".",
    "noImplicitAny": false,
    "sourceMap": false, 
    "jsx": "react",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src",
    "app",
    "app.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
